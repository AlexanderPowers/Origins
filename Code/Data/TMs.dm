obj/Machine
	Stackable = 1
	TakeDrop = 1
	Description = "A Compact Disk, used to teach a Pokemon a particular move."
	icon = 'Items.dmi'
	var/
		list/PokemonLearn = list() //A list of Pokemon that are able to learn the move..Your gonna have to type a lot!
		list/PokemonNotLearn = list()
		TypeLearn //This is if every Pokemon of this type learns the move.
		obj/MoveTaught = null //The move that will either be added to
	TM
		icon_state = "Normal TM"
		var/Inuse
		Click()
			var/list/Poke = list()
			if(Inuse) return
			Inuse = 1
			var/mob/Player/M
			if(istype(usr,/mob/Player)) M = usr
			if(!M) return

			for(var/mob/Pokemon/P in M.Party)
				if(PokemonLearn.Find(P.type)) Poke += P
			var/mob/Pokemon/P = M.Party_Choose("Teach to which Pokemon", M.Party)

			if(!P)
				Inuse = 0
				return
			for(var/obj/Moves/MM in P.Moves)
				if(istype(MM,MoveTaught))
					M<<System("[P] is unable to learn [src]")
					Inuse = 0
					return
			if(!PokemonLearn.Find(P.type) && !TypeLearn) //You've already check this, but who cares.

				M<<System("[P] is unable to learn [src]")
				Inuse = 0
				return

			else if(TypeLearn && P.Type1 != TypeLearn && P.Type2 != TypeLearn)
				M<<System("[P] is unable to learn [src] because it is not a [TypeLearn] type.")
				Inuse = 0
				return
			if(P.Moves.len >= 6) //It checks to make sure there is room to add another move, if not we have to delete one.
				if(M)
					if(alert(M,"[P] already knows 6 moves, forget one to learn [src]","","Yes","No") == "Yes")
						if(M && M.client) del input(M,"Forget which move?") in P.Moves

					else
						Inuse = 0
						return
			P.NoMoves += MoveTaught
			var/obj/Moves/MMM = new MoveTaught

			MMM.Owner = P
			P.Moves += MMM
			src.Subtract(M,1)
			Inuse = 0


	HM
		icon_state = "Water TM"
		Stackable = 0
		TakeDrop = 0
		Click()
			var/mob/Player/M
			if(istype(usr,/mob/Player)) M = usr
			if(!M) return

			var/list/Poke = list()
			for(var/mob/Pokemon/P in M.Party)
				if(PokemonLearn.Find(P.type) && !P.HM) Poke += P
			var/mob/Pokemon/P = M.Party_Choose("Teach to which Pokemon",M.Party)
			if(!P) return

			if(!PokemonLearn.Find(P.type)) //You've already check this, but who cares.
				M<<System("[P] is unable to learn [src]")
				return

			P.HM = new MoveTaught
			M<<System("[P] has learned [src].")

			M.UpdateItems()




